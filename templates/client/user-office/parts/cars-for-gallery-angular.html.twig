<ul class="breadcrumbs" ng-repeat="car in post.cars">
    <li>
        <a class="item-link" href="{{ path("homepage") }}">
            <img src="/img/Logo1.svg" alt="Парус">
        </a>
    </li>
    <li>/</li>
    <li>
        <a class="item-link" href="" ng-bind="car.brand"></a>
    </li>
    <li ng-show="car.model">/</li>
    <li ng-show="car.model">
        <a class="item-link" href="" ng-bind="car.model"></a>
    </li>
    <li ng-show="car.engineType">/</li>
    <li ng-show="car.engineType">
        <a class="current" ng-bind="car.engineType"></a>
    </li>
    <li>
        <a class="item-link-plus" href="" ng-click="gallery.removeGalleryCar(car.id)">
            <svg  version="1.1" x="0px" y="0px" width="14" height="14" viewBox="0 0 50 50" enable-background="new 0 0 50 50" xml:space="preserve">
                <line fill="none" stroke="#000000" stroke-width="3" stroke-miterlimit="10" x1="7" y1="7" x2="43" y2="43"/>
                <line fill="none" stroke="#000000" stroke-width="3" stroke-miterlimit="10" x1="43" y1="7" x2="7" y2="43"/>
            </svg>
        </a>
    </li>
</ul>