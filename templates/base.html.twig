                                            <path d="M28.3163 0L49.9999 49.9998H28.3163V0Z" fill="#494242"/>
                                        </g>
                                        <defs>
                                            <clipPath id="clip0">
                                                <rect width="50" height="50" fill="white"/>
                                            </clipPath>
                                        </defs>
                                    </svg>
                                </a>
                            </li>
                        {% endif %}

                        <li>
                            <a class="navigation-link cbutton cbutton--effect-boris" href="{{ path("show_brand_catalog_choice_brand") }}" title="Поиск">
